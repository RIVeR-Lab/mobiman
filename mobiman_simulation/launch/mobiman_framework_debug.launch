<launch>
  <!--
    config_mobiman_framework_gazebo
    config_mobiman_framework_igibson

    config_mobiman_framework_igibson_debug_mpc
    config_mobiman_framework_igibson_debug_mrt
  -->
  <arg name="config_mobiman_framework"    default="config_mobiman_framework_igibson_debug_mrt.yaml"/>
  <param name="config_mobiman_framework"  value="$(arg config_mobiman_framework)" />

  <!-- RUN MOBIMAN FRAMEWORK -->
  <!--
    mobiman_framework_launch_debug_mpc
    mobiman_framework_launch_debug_mrt
  -->
  <node name="mobiman_framework_launch_debug_mrt" pkg="mobiman_simulation" type="mobiman_framework_launch.py" output="screen" />
</launch>
